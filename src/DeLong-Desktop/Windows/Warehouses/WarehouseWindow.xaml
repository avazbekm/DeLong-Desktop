<Window x:Class="DeLong_Desktop.Windows.Warehouses.WarehouseWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeLong_Desktop.Windows.Warehouses"
        mc:Ignorable="d"
        xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="WarehouseWindow" Height="300" Width="400"
        WindowStartupLocation="CenterScreen">
    <Grid  Background="#334257">
            <StackPanel x:Name="spWarehouse" Margin="20 0 20 20" VerticalAlignment="Top">
                <TextBox x:Name="txtbName" MaxLength="50" FontSize="18" Height="30" Margin="0 10 0 5" Padding="5 0 0 0" md:HintAssist.Hint="Ombor nomi" Background="#476072" Foreground="#EEEEEE"/>
                <TextBox x:Name="txtbMudir" MaxLength="50" FontSize="18" Height="30" Margin="0 10 0 5" Padding="5 0 0 0" md:HintAssist.Hint="Ombor mudirining FIO" Background="#476072" Foreground="#EEEEEE"/>
                <TextBox x:Name="txtbAddress" MaxLength="50" FontSize="18" Height="30" Margin="0 10 0 5" Padding="5 0 0 0" md:HintAssist.Hint="Ombor manzili" Background="#476072" Foreground="#EEEEEE"/>
                <Button x:Name="btnAddWarehouse" Visibility="Visible" Content="Add Product" BorderThickness="0" Background="#548CA8" Foreground="#EEEEEE" Height="30" Margin="0 20 0 0"/>
            </StackPanel>
    </Grid>
</Window>
